  "files.associations": {
    "*.json": "json"
  },
  "files.exclude": {
    "node_modules": true,
    "bun.lock": true
  },
  "files.readonlyInclude": {},
  "files.watcherExclude": {},
  "search.exclude": {
    "bun.lock": true
  },
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.preferences.autoImportFileExcludePatterns": [],
  "typescript.preferences.importModuleSpecifier": "non-relative"
}
